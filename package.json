{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "A responsive Tic Tac Toe game with AI opponent",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "serve": "npx http-server . -p 8080"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/tests/**",
      "!**/coverage/**"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0"
  },
  "keywords": ["tic-tac-toe", "game", "ai", "responsive"],
  "author": "",
  "license": "MIT"
}